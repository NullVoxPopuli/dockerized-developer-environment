Urxvt*imLocale: en_US.UTF-8
URxvt*termName: rxvt-unicode
URxvt.perl-ext-common: default,matcher
URxvt.iso14755: False
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob

URxvt.scrollBar: false
URxvt.font: xft:DejaVu Sans Mono:size=13
URxvt.boldFont: xft:DejaVu Sans Mono:style:Bold:size=13,xft:Unifont:size=13
URxvt.ItalicFont: xft:DejaVu Sans Mono:style:Italic:size=13,xft:Unifont:size=13
URxvt.boldItalicFont: xft:DejaVu Sans Mono:style:Bold Italic:size=13,xft:Unifont:size=13
URxvt.letterSpace: -1

URxvt.depth: 32

URxvt.matcher.button: 1
URxvt.url-select.underline: true
URxvt.geometry: 100x24
